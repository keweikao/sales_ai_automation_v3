{
  "name": "@sales_ai_automation_v3/shared",
  "version": "0.2.0",
  "description": "Shared types, errors, and schemas for Sales AI Automation V3",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./errors": {
      "types": "./src/errors/index.ts",
      "import": "./src/errors/index.ts"
    },
    "./product-configs": {
      "types": "./src/product-configs/index.ts",
      "import": "./src/product-configs/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "import": "./src/types/index.ts"
    },
    "./types/conversation": {
      "types": "./src/types/conversation/index.ts",
      "import": "./src/types/conversation/index.ts"
    },
    "./types/transcription": {
      "types": "./src/types/transcription/index.ts",
      "import": "./src/types/transcription/index.ts"
    },
    "./types/meddic": {
      "types": "./src/types/meddic/index.ts",
      "import": "./src/types/meddic/index.ts"
    },
    "./types/slack": {
      "types": "./src/types/slack/index.ts",
      "import": "./src/types/slack/index.ts"
    },
    "./types/queue": {
      "types": "./src/types/queue/index.ts",
      "import": "./src/types/queue/index.ts"
    },
    "./types/opportunity": {
      "types": "./src/types/opportunity/index.ts",
      "import": "./src/types/opportunity/index.ts"
    },
    "./schemas": {
      "types": "./src/schemas/index.ts",
      "import": "./src/schemas/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "bun x ultracite check",
    "fix": "bun x ultracite fix"
  },
  "dependencies": {
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:"
  },
  "keywords": [
    "shared",
    "types",
    "errors",
    "schemas"
  ],
  "author": "",
  "license": "ISC"
}
